[Desktop Entry]
_Name=Warlock Bar
Type=Application
Exec=@BINDIR@/@PACKAGE_NAME@
Terminal=false
Icon=@PIXMAPDIR@/@PACKAGE_NAME@.png
_Comment=A light and fast launch bar.
Categories=Utility;
